<h1>{{{title}}}</h1>

<table class="table">
    <thead>
        <tr>
            <th>id</th>
            <th>manufacturer</th>
            <th>manufacturerid</th>
            <th>product</th>
            <th>producttype</th>
            <th>productid</th>
            <th>type</th>
            <th>name</th>
            <th>loc</th>
        </tr>
    </thead>

    <tbody>

        {{# each nodeCollection }}
            <tr>
                <td>{{nodeid}}</td>
                <td>{{manufacturer}}</td>
                <td>{{manufacturerid}}</td>
                <td>{{product}}</td>
                <td>{{producttype}}</td>
                <td>{{productid}}</td>
                <td>{{type}}</td>
                <td>{{name}}</td>
                <td>{{loc}}</td>
            </tr>
        {{/ each }}

    </tbody>
</table>

<button type="button" class="btn btn-primary js-refresh">
    Refresh lijst
</button>
